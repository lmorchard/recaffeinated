    <item>
        <title>[^get(Prototype)^] ^title^</title>
        ^if(^get(HasOwnPage)^)^
        <link>^host^^path^</link>
        <guid>^host^^path^</guid>
        ^else^
        <link>^host^^path(parent)^#entry-^get(id)^</link>
        <guid>^host^^path(parent)^#entry-^get(id)^</guid>
        ^endif^
        ^if(^text^)^
        <description><![CDATA[^text^]]></description>
        ^endif^
        <pubDate>^lastModified(this,*)^</pubDate>
    </item>
