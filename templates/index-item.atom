      <entry>
        <title>[^get(Prototype)^] ^title^</title>
        ^if(^get(HasOwnPage)^)^
        <id>^host^^path^</id>
        <link href="^host^^path^" type="text/html" />
        ^else^
        <id>^host^^path(parent)^#entry-^getFor(this,id)^</id>
        <link href="^host^^path(parent)^#entry-^getFor(this,id)^" type="text/html" rel="alternate" />
        ^endif^
        <updated>^lastModified(this,=)^</updated>
        <summary type="html">^if(^text^)^<![CDATA[^text^]]>^endif^</summary>
      </entry>
